          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "WITH cpu_rates AS (\n  SELECT\n    time,\n    metric_name,\n    (value - LAG(value) OVER (PARTITION BY metric_name ORDER BY time)) /\n    NULLIF(EXTRACT(EPOCH FROM time - LAG(time) OVER (PARTITION BY metric_name ORDER BY time)), 0) AS rate\n  FROM metrics\n  WHERE container_name = 'host'\n    AND metric_name LIKE 'host_cpu_seconds_total_%'\n    AND $__timeFilter(time)\n),\nbucketed AS (\n  SELECT\n    time_bucket('5s', time) AS bucket,\n    metric_name,\n    AVG(rate) AS avg_rate\n  FROM cpu_rates\n  WHERE rate IS NOT NULL AND rate >= 0\n  GROUP BY time_bucket('5s', time), metric_name\n)\nSELECT\n  bucket AS time,\n  'CPU Usage' AS metric,\n  100 - (SUM(CASE WHEN metric_name = 'host_cpu_seconds_total_idle' THEN avg_rate ELSE 0 END) /\n         NULLIF(SUM(avg_rate), 0) * 100) AS value\nFROM bucketed\nGROUP BY bucket\nORDER BY bucket",
          "refId": "A",
          "select": [
            [
              {
                "params": [
                  "value"
                ],
                "type": "column"
              }
            ]
          ],
          "timeColumn": "time",
          "where": [
            {
              "name": "$__timeFilter",
              "params": [],
              "type": "macro"
            }
          ]
        }
      ],
      "title": "Host / VM CPU Usage %",
      "type": "timeseries"
